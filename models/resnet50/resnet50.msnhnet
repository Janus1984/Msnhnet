      batchNorm: 0
      filters: 64
      kSize: 1
      stride: 1
      padding: 0
      useBias: 0
      activation: none

    batchnorm:
      activation: relu

    conv:
      batchNorm: 0
      filters: 64
      kSize: 3
      stride: 1
      padding: 1
      useBias: 0
      activation: none

    batchnorm:
