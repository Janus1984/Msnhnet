config:
  batch: 1
  width: 224
  height: 224
  channels: 3

#0-1
conv:
  batchNorm: 0
  filters: 64
  kSize: 7
  stride: 2
  padding: 3
  useBias: 0
  activation: none
batchnorm:
  activation: relu

maxpool:
  kSize: 3
  stride: 2
  padding: 1

# Layer1
res2block:
