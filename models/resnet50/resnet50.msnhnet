      batchNorm: 0
      filters: 256
      kSize: 1
      stride: 1
      padding: 0
      useBias: 0
      activation: none

    batchnorm:
      activation: none

  activation: relu

resblock:
  size: 2
  conv:
    batchNorm: 0
    filters: 64
    kSize: 1
    stride: 1
    padding: 0
    useBias: 0
    activation: none

  batchnorm:
